			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "CustomerSourceInput",
								"type": "DatasetReference"
							},
							"name": "CustomersInput"
						},
